{
  "manifest_version": 3,
  "name": "Telegram Sender",
  "version": "1.0",
  "description": "Send selected text to Telegram with anodra.uz",
  "permissions": ["contextMenus", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "options_page": "options.html"
}
